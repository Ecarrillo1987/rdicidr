{
  "family": "fsl-frontend",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::789891675707:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "fsl-frontend",
      "image": "789891675707.dkr.ecr.us-east-1.amazonaws.com/fsl-frontend:1.0",
      "essential": true,
      "portMappings": [
        { "containerPort": 8080, "protocol": "tcp" }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/fsl-frontend",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ]
}